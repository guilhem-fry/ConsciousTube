{
  "manifest_version": 3,
  "name": "YouTube Intention",
  "version": "1.0",
  "description": "Promotes intentional YouTube browsing",
  "permissions": ["webNavigation", "scripting"],
  "host_permissions": ["*://www.youtube.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}

